{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "Compilar y Ejecutar StudentManager",
			"type": "shell",
			"command": "gcc",
			"args": [
				"-o",
				"StudentManager.exe",
				"main.c",
				"estudiante.c",
				"archivo.c",
				"-std=c99",
				"-lcomdlg32",
				"&&",
				"./StudentManager.exe"
			],
			"group": "build",
			"problemMatcher": [
				"$gcc"
			],
			"isBackground": false
		}
	]
}